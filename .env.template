# Tailscale MCP Configuration
# Copy this file to .env and fill in your values

# Your Tailscale API token (required)
# Get this from: https://login.tailscale.com/admin/settings/keys
TAILSCALE_TOKEN=your_tailscale_api_token_here

# Your Tailnet name (optional, defaults to "default")
# This is usually your organization name or email domain
TAILNET=your_tailnet_name
